{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a YOUR_HEROKU_APP_NAME",
    "rollbackh": "heroku run knex migrate:rollback -a YOUR_HEROKU_APP_NAME",
    "databaseh": "heroku pg:psql -a YOUR_HEROKU_APP_NAME",
    "seedh": "heroku run knex seed:run -a YOUR_HEROKU_APP_NAME",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  },
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1010.0",
    "bcryptjs": "^2.4.3",
    "connect-session-knex": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "knex-cleaner": "^1.3.1",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
    "pg": "8.7.1",
    "sqlite3": "^5.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "jest": "27.0.6",
    "nodemon": "^2.0.12",
    "supertest": "6.1.6"
  }
}
